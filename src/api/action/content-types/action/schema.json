{
  "kind": "collectionType",
  "collectionName": "actions",
  "info": {
    "singularName": "action",
    "pluralName": "actions",
    "displayName": "Action",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "NO_COST",
        "LOW_COST",
        "INVESTMENT"
      ],
      "required": true
    },
    "impact": {
      "type": "enumeration",
      "enum": [
        "LOW",
        "MEDIUM",
        "HIGH"
      ],
      "required": true
    },
    "effort": {
      "type": "enumeration",
      "enum": [
        "EASY",
        "AVERAGE",
        "DIFFICULT"
      ],
      "required": true
    },
    "popular": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "PRODUCT",
        "SERVICE",
        "HABIT"
      ],
      "required": true
    },
    "thumbnail": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "active": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "ctaUrl": {
      "type": "string"
    }
  }
}
